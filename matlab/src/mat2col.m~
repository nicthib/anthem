out = ['[' num2str(in(1))];

for i = 1:numel(tmp)
    if tmp(i) > 1 && strcmp(out(end),':')
        out = [out ':' in(i)];
    elseif tmp(i) > 1
        out = [out ', '];
    elseif tmp(i) == 1
        
    end
end
out = [out ']'];